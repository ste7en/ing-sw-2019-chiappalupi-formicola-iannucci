<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>AM19</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <useProjectArtifact>false</useProjectArtifact>
            <unpack>true</unpack>
            <scope>runtime</scope>
            <includes>
                <include>com.fasterxml.jackson.core:jackson*</include>
            </includes>

        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <directory>${project.build.outputDirectory}</directory>
            <outputDirectory>/</outputDirectory>
            <excludes>
                <exclude>*javafx*</exclude>
                <exclude>*jfx*</exclude>
                <exclude>images/**</exclude>
                <exclude>assemblies/**</exclude>
            </excludes>
        </fileSet>
    </fileSets>
</assembly>